{
  "name": "techstore-ecommerce",
  "version": "1.0.0",
  "description": "Professional e-commerce platform with Node.js backend",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "dev": "nodemon server/app.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "build": "npm run build:css && npm run build:js",
    "build:css": "postcss public/css/style.css -o public/css/style.min.css",
    "build:js": "webpack --mode production",
    "lint": "eslint server/ public/js/",
    "lint:fix": "eslint server/ public/js/ --fix",
    "deploy": "gh-pages -d public"
  },
  "keywords": [
    "ecommerce",
    "nodejs",
    "express",
    "mongodb",
    "payment",
    "mpesa",
    "shopping-cart"
  ],
  "author": "Yusuph Lameck <yusuphlameck220@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.4",
    "multer": "^1.4.5-lts.1",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "connect-mongo": "^5.0.0",
    "socket.io": "^4.7.2",
    "moment": "^2.29.4",
    "uuid": "^9.0.0",
    "axios": "^1.5.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "postcss": "^8.4.27",
    "postcss-cli": "^10.1.0",
    "autoprefixer": "^10.4.15",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "gh-pages": "^6.0.0",
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/techstore.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/techstore/issues"
  },
  "homepage": "https://yourusername.github.io/techstore"
}